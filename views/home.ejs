  <main role="main" class="container  container body-content">
    <div class="jumbotron " align="center">
      <div class="search">
        <form action="/" method="post" class="form-inline mt-2 mt-md-0">
          <input class="form-control mr-sm-2" name="recipe" type="text" placeholder="Enter Recipe" aria-label="Search">

          <button class="btn btn-lg  btn-success my-2 my-sm-0" type="submit">Search</button>
        </form>
