        <h2 class="featurette-heading"><%=recipe.title%></h2>
        <p><strong>Ready in:</strong> <%= recipe.readyInMinutes%> minutes</p>
        <p><a class="btn btn-secondary" href="#" role="button">View details Â»</a></p>
      </div><!-- /.col-lg-4 -->
      <%})%>
